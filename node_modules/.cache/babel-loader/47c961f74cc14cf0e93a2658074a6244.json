EAAA,KApBA;AAqBA,MAAA,QAAA,EAAA,KArBA;AAsBA,MAAA,MAAA,EAAA,CAAA;AACA,QAAA,KAAA,EAAA,EADA;AAEA,QAAA,GAAA,EAAA,UAFA;AAGA,QAAA,QAAA,EAAA;AAHA,OAAA,EAKA;AACA,QAAA,GAAA,EAAA,MADA;AAEA,QAAA,QAAA,EAAA;AAFA,OALA,EASA;AACA,QAAA,GAAA,EAAA,KADA;AAEA,QAAA,QAAA,EAAA;AAFA,OATA,EAaA;AACA,QAAA,GAAA,EAAA,QADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAbA,EAiBA;AACA,QAAA,GAAA,EAAA,UADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAjBA,EAqBA;AACA,QAAA,GAAA,EAAA,OADA;AAEA,QAAA,QAAA,EAAA;AAFA,OArBA,EAyBA;AACA,QAAA,GAAA,EAAA;AADA,OAzBA;AAtBA,KAAA;AAoDA,GA3DA;AA4DA,EAAA,QAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,IAJA,kBAIA;AACA,aAAA,KAAA,SAAA,CAAA,MAAA;AACA;AANA,GA5DA;AAoEA,EAAA,OApEA,qBAoEA;AACA;AACA,SAAA,SAAA,GAAA,KAAA,KAAA,CAAA,MAAA;AACA,IAAA,KAAA,CACA,GADA,CACA,kDADA,EACA;AACA,MAAA,OAAA,EAAA;AACA,wBAAA;AADA;AADA,KADA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,aAAA,GAAA;AACA,KARA;AASA,GAhFA;AAiFA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,UAJA,sBAIA,aAJA,EAIA;AACA;AACA,WAAA,SAAA,GAAA,aAAA,CAAA,MAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,KARA;AASA,IAAA,QAAA,EAAA,oBAAA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,QAAA,GAAA,EAAA;;AACA,UAAA,KAAA,UAAA,EAAA;AAAA;AACA,aAAA,IAAA,GAAA,IAAA,KAAA,SAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,KAAA,SAAA,CAAA,GAAA,CAAA;AACA;AACA;AACA,KAjBA;AAkBA,IAAA,cAAA,EAAA,0BAAA;AACA,UAAA,KAAA,QAAA,CAAA,MAAA,IAAA,KAAA,SAAA,CAAA,MAAA,EAAA;AACA,aAAA,UAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,UAAA,GAAA,KAAA;AACA;AACA;AAxBA;AAjFA,CAAA","sourcesContent":["<script>\r\nimport Layout from \"../../../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport axios from \"axios\";\r\nimport { pagesData } from \"./pages-data\";\r\nimport appConfig from \"@/app.config\";\r\n\r\n/**\r\n * Pages component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Pages\",\r\n    meta: [{ name: \"description\", content: appConfig.description }]\r\n  },\r\n  components: { Layout, PageHeader },\r\n  data() {\r\n    return {\r\n      pagesData: pagesData,\r\n      title: \"Pages\",\r\n      items: [\r\n        {\r\n          text: \"Content\",\r\n        },\r\n        {\r\n          text: \"Pages\",\r\n          active: true\r\n        }\r\n      ],\r\n      totalRows: 1,\r\n      currentPage: 1,\r\n      perPage: 10,\r\n      pageOptions: [10, 25, 50, 100],\r\n      filter: null,\r\n      filterOn: [],\r\n      selected: [],\r\n      isCheckAll: false,\r\n      sortBy: \"age\",\r\n      sortDesc: false,\r\n      fields: [{\r\n              label: \"\",\r\n              key: \"selected\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"name\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"url\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"status\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"modified\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"added\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"actions\",\r\n          },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n      /**\r\n        * Total no. of records\r\n        */\r\n      rows() {\r\n          return this.pagesData.length;\r\n      },\r\n  },\r\n  mounted() {\r\n      // Set the initial number of items\r\n      this.totalRows = this.items.length;\r\n      axios\r\n          .get(\"http://dummy.restapiexample.com/api/v1/employees\", {\r\n              headers: {\r\n                  \"Content-type\": \"application/json;charset=utf-8\",\r\n              },\r\n          })\r\n          .then((res) => {\r\n              return res;\r\n          });\r\n  },\r\n  methods: {\r\n      /**\r\n        * Search the table data with search input\r\n        */\r\n      onFiltered(filteredItems) {\r\n          // Trigger pagination to update the number of buttons/pages due to filtering\r\n          this.totalRows = filteredItems.length;\r\n          this.currentPage = 1;\r\n      },\r\n      checkAll: function(){\r\n        this.isCheckAll = !this.isCheckAll;\r\n        this.selected = [];\r\n        if(this.isCheckAll){ // Check all\r\n          for (var key in this.pagesData) {\r\n            this.selected.push(this.pagesData[key]);\r\n          }\r\n        }\r\n      },\r\n      updateCheckall: function(){\r\n        if(this.selected.length == this.pagesData.length){\r\n          this.isCheckAll = true;\r\n        }else{\r\n          this.isCheckAll = false;\r\n        }\r\n      },\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"row mb-2\">\r\n              <div class=\"col-sm-4\">\r\n                \r\n              </div>\r\n              <div class=\"col-sm-8\">\r\n                <div class=\"text-sm-right\">\r\n                  <a href=\"/content/pages/add\">\r\n                    <button type=\"button\" class=\"btn btn-success btn-rounded mb-2 mr-2\">\r\n                      <i class=\"mdi mdi-plus mr-1\"></i> Add Page\r\n                    </button>\r\n                  </a>\r\n                </div>\r\n              </div>\r\n              <!-- end col-->\r\n            </div>\r\n            <div class=\"row mt-4\">\r\n                      <div class=\"col-sm-12 col-md-6\">\r\n                        <div id=\"tickets-table_length\" class=\"dataTables_length\">\r\n                            <label class=\"d-inline-flex align-items-center\">\r\n                                Show&nbsp;\r\n                                <b-form-select v-model=\"perPage\" size=\"sm\" :options=\"pageOptions\"></b-form-select>&nbsp;entries\r\n                            </label>\r\n                        </div>\r\n                      </div>\r\n                      <!-- Search -->\r\n                      <div class=\"col-sm-12 col-md-6\">\r\n                          <div id=\"tickets-table_filter\" class=\"dataTables_filter text-md-right\">\r\n                              <label class=\"d-inline-flex align-items-center\">Search:\r\n                                  <b-form-input v-model=\"filter\" type=\"search\" placeholder=\"Search...\" class=\"form-control form-control-sm ml-2\"></b-form-input>\r\n                              </label>\r\n                          </div>\r\n                      </div>\r\n                      <!-- End search -->\r\n                  <!-- Table -->\r\n                    <div class=\"table-responsive mb-0\">\r\n                        <b-table :items=\"pagesData\" selectable :fields=\"fields\" responsive=\"sm\" :per-page=\"perPage\" :current-page=\"currentPage\" :sort-by.sync=\"sortBy\" :sort-desc.sync=\"sortDesc\" :filter=\"filter\" :filter-included-fields=\"filterOn\" @filtered=\"onFiltered\">\r\n                          <template #head(selected)=\"data\">\r\n                            <b-form-checkbox\r\n                            @click=\"checkAll()\"\r\n                            v-model=\"isCheckAll\"\r\n                            :value=\"data.id\"\r\n                            unchecked-value=\"not_accepted\"\r\n                            class=\"custom-checkbox custom-checkbox-outline custom-checkbox-primary\"\r\n                            checked\r\n                          ></b-form-checkbox>\r\n                          </template>\r\n                          <template #cell(selected)=\"data\">\r\n                            <b-form-checkbox\r\n                            v-model=\"primaryoutlinecheck\"\r\n                            v-bind:value='data.id' :v-model='data.id'\r\n                            @change=\"updateCheckall()\"\r\n                            unchecked-value=\"not_accepted\"\r\n                            class=\"custom-checkbox custom-checkbox-outline custom-checkbox-primary mb-3\"\r\n                            checked\r\n                          ></b-form-checkbox>\r\n                          </template>\r\n                          <template #cell(actions)=\"data\">\r\n                            <b-dropdown class=\"card-drop\" variant=\"white\" right toggle-class=\"p-0\">\r\n                              <template v-slot:button-content>\r\n                                <i class=\"mdi mdi-dots-horizontal font-size-18\"></i>\r\n                              </template>\r\n\r\n                              <b-dropdown-item href=\"/content/blocks/edit\">\r\n                                <i class=\"fas fa-pencil-alt text-success mr-1\"></i> Edit\r\n                              </b-dropdown-item>\r\n\r\n                              <b-dropdown-item>\r\n                                <i class=\"fas fa-trash-alt text-danger mr-1\"></i> Delete\r\n                              </b-dropdown-item>\r\n                            </b-dropdown>\r\n                          </template>\r\n                        </b-table>\r\n                    </div>\r\n                    <div class=\"row\">\r\n                        <div class=\"col\">\r\n                            <div class=\"dataTables_paginate paging_simple_numbers float-right\">\r\n                                <ul class=\"pagination pagination-rounded mb-0\">\r\n                                    <!-- pagination -->\r\n                                    <b-pagination v-model=\"currentPage\" :total-rows=\"rows\" :per-page=\"perPage\"></b-pagination>\r\n                                </ul>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                  </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"],"sourceRoot":"src/router/views/content/pages"}]}