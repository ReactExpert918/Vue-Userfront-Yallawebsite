A,EAAA,EAAA,EAAA,EAAA,EAAA,GAAA,CAtBA;AAuBA,MAAA,MAAA,EAAA,IAvBA;AAwBA,MAAA,QAAA,EAAA,EAxBA;AAyBA,MAAA,MAAA,EAAA,KAzBA;AA0BA,MAAA,QAAA,EAAA,KA1BA;AA2BA,MAAA,MAAA,EAAA,CAAA;AACA,QAAA,KAAA,EAAA,UADA;AAEA,QAAA,GAAA,EAAA,UAFA;AAGA,QAAA,QAAA,EAAA;AAHA,OAAA,EAKA;AACA,QAAA,GAAA,EAAA,MADA;AAEA,QAAA,QAAA,EAAA;AAFA,OALA,EASA;AACA,QAAA,GAAA,EAAA,KADA;AAEA,QAAA,QAAA,EAAA;AAFA,OATA,EAaA;AACA,QAAA,GAAA,EAAA,OADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAbA,EAiBA;AACA,QAAA,GAAA,EAAA,KADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAjBA,EAqBA;AACA,QAAA,GAAA,EAAA,QADA;AAEA,QAAA,QAAA,EAAA;AAFA,OArBA;AA3BA,KAAA;AAsDA,GA7DA;AA8DA,EAAA,QAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,IAJA,kBAIA;AACA,aAAA,KAAA,eAAA,CAAA,MAAA;AACA;AANA,GA9DA;AAsEA,EAAA,OAtEA,qBAsEA;AACA;AACA,SAAA,SAAA,GAAA,KAAA,KAAA,CAAA,MAAA;AACA,IAAA,KAAA,CACA,GADA,CACA,kDADA,EACA;AACA,MAAA,OAAA,EAAA;AACA,wBAAA;AADA;AADA,KADA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,aAAA,GAAA;AACA,KARA;AASA,GAlFA;AAmFA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,UAJA,sBAIA,aAJA,EAIA;AACA;AACA,WAAA,SAAA,GAAA,aAAA,CAAA,MAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA;AARA;AAnFA,CAAA","sourcesContent":["<script>\r\nimport Layout from \"../../../../../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport axios from \"axios\";\r\nimport { allProductsData } from \"./add-data\";\r\nimport appConfig from \"@/app.config\";\r\n\r\n/**\r\n * Pages component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Add Products\",\r\n    meta: [{ name: \"description\", content: appConfig.description }]\r\n  },\r\n  components: { Layout, PageHeader },\r\n  data() {\r\n    return {\r\n      allProductsData: allProductsData,\r\n      selectedToogle: \"A\",\r\n      title: \"Add Products\",\r\n      items: [\r\n        {\r\n          text: \"Sales\",\r\n        },\r\n        {\r\n          text: \"Orders\",\r\n        },\r\n        {\r\n          text: \"Edit Order\",\r\n        },\r\n        {\r\n          text: \"Add Products\",\r\n          active: true\r\n        }\r\n      ],\r\n      totalRows: 1,\r\n      currentPage: 1,\r\n      perPage: 10,\r\n      pageOptions: [10, 25, 50, 100],\r\n      filter: null,\r\n      filterOn: [],\r\n      sortBy: \"age\",\r\n      sortDesc: false,\r\n      fields: [{\r\n              label: \"Selected\",\r\n              key: \"selected\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"name\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"sku\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"price\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"qty\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"status\",\r\n              sortable: true,\r\n          },\r\n      ],\r\n    };\r\n  },\r\n    computed: {\r\n      /**\r\n        * Total no. of records\r\n        */\r\n      rows() {\r\n          return this.allProductsData.length;\r\n      },\r\n  },\r\n  mounted() {\r\n      // Set the initial number of items\r\n      this.totalRows = this.items.length;\r\n      axios\r\n          .get(\"http://dummy.restapiexample.com/api/v1/employees\", {\r\n              headers: {\r\n                  \"Content-type\": \"application/json;charset=utf-8\",\r\n              },\r\n          })\r\n          .then((res) => {\r\n              return res;\r\n          });\r\n  },\r\n  methods: {\r\n      /**\r\n        * Search the table data with search input\r\n        */\r\n      onFiltered(filteredItems) {\r\n          // Trigger pagination to update the number of buttons/pages due to filtering\r\n          this.totalRows = filteredItems.length;\r\n          this.currentPage = 1;\r\n      },\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"row mb-2\">\r\n              <div class=\"col-sm-12\">\r\n                <div class=\"text-sm-right\">\r\n                  <button type=\"button\" class=\"btn btn btn-rounded mb-2 mr-2\">\r\n                    <i class=\"mdi mdi-trash mr-1\"></i> Back\r\n                  </button>\r\n                  <button type=\"button\" class=\"btn btn-success btn-rounded mb-2 mr-2\">\r\n                    <i class=\"mdi mdi-plus mr-1\"></i> Save\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <!-- end col-->\r\n            </div>\r\n            <div class=\"row card-body\">\r\n              <div class=\"col-sm-12\">\r\n                <div class=\"card-body\">\r\n                    <h3>All Products</h3>\r\n                    <div class=\"row mt-4\">\r\n                      <div class=\"col-sm-12 col-md-6\">\r\n                        <div id=\"tickets-table_length\" class=\"dataTables_length\">\r\n                            <label class=\"d-inline-flex align-items-center\">\r\n                                Show&nbsp;\r\n                                <b-form-select v-model=\"perPage\" size=\"sm\" :options=\"pageOptions\"></b-form-select>&nbsp;entries\r\n                            </label>\r\n                        </div>\r\n                      </div>\r\n                      <!-- Search -->\r\n                      <div class=\"col-sm-12 col-md-6\">\r\n                          <div id=\"tickets-table_filter\" class=\"dataTables_filter text-md-right\">\r\n                              <label class=\"d-inline-flex align-items-center\">\r\n                                  Search:\r\n                                  <b-form-input v-model=\"filter\" type=\"search\" placeholder=\"Search...\" class=\"form-control form-control-sm ml-2\"></b-form-input>\r\n                              </label>\r\n                          </div>\r\n                      </div>\r\n                      <!-- End search -->\r\n                    </div>\r\n                  <!-- Table -->\r\n                    <div class=\"table-responsive mb-0\">\r\n                        <b-table :items=\"allProductsData\" selectable :fields=\"fields\" responsive=\"sm\" :per-page=\"perPage\" :current-page=\"currentPage\" :sort-by.sync=\"sortBy\" :sort-desc.sync=\"sortDesc\" :filter=\"filter\" :filter-included-fields=\"filterOn\" @filtered=\"onFiltered\">\r\n                      <template #cell(selected)=\"data\">\r\n                        <b-form-checkbox switch size=\"lg\" v-model=\"lgchecked\"></b-form-checkbox>\r\n                      </template>\r\n                        </b-table>\r\n                    </div>\r\n                    <div class=\"row\">\r\n                        <div class=\"col\">\r\n                            <div class=\"dataTables_paginate paging_simple_numbers float-right\">\r\n                                <ul class=\"pagination pagination-rounded mb-0\">\r\n                                    <!-- pagination -->\r\n                                    <b-pagination v-model=\"currentPage\" :total-rows=\"rows\" :per-page=\"perPage\"></b-pagination>\r\n                                </ul>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row card-body\">\r\n              <div class=\"col-sm-12 text-sm-right\">\r\n                <b-button variant=\"primary\">\r\n                    <i class=\"bx bx-check-double font-size-16 align-middle mr-2\"></i>\r\n                    Save\r\n                </b-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"],"sourceRoot":"src/router/views/sales/orders/edit/add"}]}