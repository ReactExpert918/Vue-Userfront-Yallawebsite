   <th>Purchase Date</th>\r\n                    <th>Ship To</th>\r\n                    <th>Total</th>\r\n                    <th>Status</th>\r\n                    <th>Actions</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"order in ordersData\" :key=\"order.id\">\r\n                    <td>\r\n                      <div class=\"custom-control custom-checkbox\">\r\n                        <input\r\n                          :id=\"`customCheck${order.id}`\"\r\n                          type=\"checkbox\"\r\n                          class=\"custom-control-input\"\r\n                        />\r\n                        <label\r\n                          class=\"custom-control-label\"\r\n                          :for=\"`customCheck${order.id}`\"\r\n                        >&nbsp;</label>\r\n                      </div>\r\n                    </td>\r\n                    <td>{{order.id}}</td>\r\n                    <td>{{order.purchaseDate}}</td>\r\n                    <td>{{order.shipToName}}</td>\r\n                    <td>{{order.total}}</td>\r\n                    <td>\r\n                      <span :class=\"'badge badge-success font-size-12 ' + order.status\">\r\n                        <i class=\"mdi mdi-star mr-1\"></i>\r\n                        {{order.status}}\r\n                      </span>\r\n                    </td>\r\n                    <td>\r\n                      <b-dropdown class=\"card-drop\" variant=\"white\" right toggle-class=\"p-0\">\r\n                        <template v-slot:button-content>\r\n                          <i class=\"mdi mdi-dots-horizontal font-size-18\"></i>\r\n                        </template>\r\n\r\n                        <b-dropdown-item>\r\n                          <i class=\"fas fa-pencil-alt text-success mr-1\"></i> Edit\r\n                        </b-dropdown-item>\r\n\r\n                        <b-dropdown-item>\r\n                          