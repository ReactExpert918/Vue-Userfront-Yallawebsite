{"remainingRequest":"/Users/michaelfigg/Documents/GitHub/yallastoreadmin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/michaelfigg/Documents/GitHub/yallastoreadmin/src/router/views/catalog/products/add/add.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/michaelfigg/Documents/GitHub/yallastoreadmin/src/router/views/catalog/products/add/add.vue","mtime":1610737838613},{"path":"/Users/michaelfigg/Documents/GitHub/yallastoreadmin/node_modules/cache-loader/dist/cjs.js","mtime":1606587962276},{"path":"/Users/michaelfigg/Documents/GitHub/yallastoreadmin/node_modules/babel-loader/lib/index.js","mtime":1606588962495},{"path":"/Users/michaelfigg/Documents/GitHub/yallastoreadmin/node_modules/cache-loader/dist/cjs.js","mtime":1606587962276},{"path":"/Users/michaelfigg/Documents/GitHub/yallastoreadmin/node_modules/vue-loader/lib/index.js","mtime":1606587964121}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["add.vue"],"names":[],"mappings":";AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"add.vue","sourceRoot":"src/router/views/catalog/products/add","sourcesContent":["<script>\r\nimport CKEditor from \"@ckeditor/ckeditor5-vue\";\r\nimport ClassicEditor from \"@ckeditor/ckeditor5-build-classic\";\r\nimport vue2Dropzone from \"vue2-dropzone\";\r\nimport Multiselect from \"vue-multiselect\";\r\n\r\nimport Layout from \"../../../../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\n\r\nimport { addData } from \"./add-data\";\r\nimport { variationsData } from \"./variations-data\";\r\nimport axios from \"axios\";\r\nimport { bundleData } from \"./bundle-products\";\r\nimport appConfig from \"@/app.config\";\r\n\r\n/**\r\n * Pages component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Add Product\",\r\n    meta: [{ name: \"description\", content: appConfig.description }]\r\n  },\r\n  components: { Layout, PageHeader, ckeditor: CKEditor.component, vueDropzone: vue2Dropzone, Multiselect },\r\n  data() {\r\n    return {\r\n      addData: addData,\r\n\r\n      variations: [],\r\n      variationValue: 'Variation Name',\r\n      variationOptions: [\"value 1\", \"value 2\", \"value 3\", \"value 4\"],\r\n      variationRequired: false,\r\n\r\n      variationsData: variationsData,\r\n      bundleData: bundleData,\r\n      title: \"Add Product\",\r\n      items: [\r\n        {\r\n          text: \"Catalog\",\r\n        },\r\n        {\r\n          text: \"Products\",\r\n          href: \"/content/pages\"\r\n        },\r\n        {\r\n          text: \"Add Product\",\r\n          active: true\r\n        }\r\n      ],\r\n      attributevalues: [\r\n        \"value 1\",\r\n        \"value 2\",\r\n        \"value 3\",\r\n        \"value 4\",\r\n      ],\r\n      categoryvalues: [\r\n        \"cat 1\",\r\n        \"cat 2\",\r\n        \"cat 3\",\r\n        \"cat 4\",\r\n      ],\r\n      totalRows: 1,\r\n      currentPage: 1,\r\n      perPage: 10,\r\n      pageOptions: [10, 25, 50, 100],\r\n      filter: null,\r\n      filterOn: [],\r\n      sortBy: \"age\",\r\n      sortDesc: false,\r\n      fields: [{\r\n              label: \"Selected\",\r\n              key: \"selected\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"name\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"sku\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"price\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"qty\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"status\",\r\n              sortable: true,\r\n          },\r\n      ],\r\n      editor: ClassicEditor,\r\n      editorData: \"\",\r\n      content: \"\",\r\n      plugins: [\r\n        \"advlist autolink link image lists charmap print preview hr anchor pagebreak spellchecker\",\r\n        \"searchreplace wordcount visualblocks visualchars code fullscreen insertdatetime media nonbreaking\",\r\n        \"save table contextmenu directionality emoticons template paste textcolor\",\r\n      ],\r\n      toolbar: \"insertfile undo redo | styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | l      ink image | print preview media fullpage | forecolor backcolor emoticons\",\r\n      options: {\r\n        height: 300,\r\n        style_formats: [\r\n          { title: \"Bold text\", inline: \"b\" },\r\n          { title: \"Red text\", inline: \"span\", styles: { color: \"#ff0000\" } },\r\n          { title: \"Red header\", block: \"h1\", styles: { color: \"#ff0000\" } },\r\n          { title: \"Example 1\", inline: \"span\", classes: \"example1\" },\r\n          { title: \"Example 2\", inline: \"span\", classes: \"example2\" },\r\n          { title: \"Table styles\" },\r\n          { title: \"Table row 1\", selector: \"tr\", classes: \"tablerow1\" },\r\n        ],\r\n      },\r\n      dropzoneOptions: {},\r\n      textarea: '',\r\n      lgchecked: '',\r\n      value1: '',\r\n      outputUrl: '',\r\n\r\n    };\r\n\r\n\r\n  },\r\n  computed: {\r\n      /**\r\n        * Total no. of records\r\n        */\r\n      rows() {\r\n          return this.bundleData.length;\r\n      },\r\n  },\r\n  mounted() {\r\n      // Set the initial number of items\r\n      this.totalRows = this.items.length;\r\n      axios\r\n          .get(\"http://dummy.restapiexample.com/api/v1/employees\", {\r\n              headers: {\r\n                  \"Content-type\": \"application/json;charset=utf-8\",\r\n              },\r\n          })\r\n          .then((res) => {\r\n              return res;\r\n          });\r\n  },\r\n  methods: {\r\n      /**\r\n        * Search the table data with search input\r\n        */\r\n      onFiltered(filteredItems) {\r\n          // Trigger pagination to update the number of buttons/pages due to filtering\r\n          this.totalRows = filteredItems.length;\r\n          this.currentPage = 1;\r\n      },\r\n      addVariation(){\r\n        let variation = {\r\n          variationValue: this.variationValue,\r\n          variationOptions: this.variationOptions[this.variations.length],\r\n          variationRequired: this.variationRequired\r\n        }\r\n        this.variations.push(variation)\r\n      },\r\n      deleteVariation(index){\r\n        this.variations.splice(index, 1);\r\n      }\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-9\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n          <h4 class=\"card-title mt-3\">General</h4>\r\n            <div class=\"row\">\r\n              <div class=\"col-9\">\r\n                <label class=\"mt-3\">Product Name</label>\r\n                <b-form-input for=\"text\" value=\"Product Name\"></b-form-input>\r\n              </div>\r\n              <div class=\"col-3\">\r\n              <label class=\"mt-3\">Product Type</label>\r\n                <select class=\"custom-select\">\r\n                  <option value=\"0\">Simple Product</option>\r\n                  <option value=\"1\">Downloadable Product</option>\r\n                  <option value=\"2\">Groupped Product</option>\r\n                  <option value=\"3\">Bundled Product</option>\r\n                </select>\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <label class=\"mt-3\">Product Price</label>\r\n                <b-form-input for=\"text\" value=\"Product Name\"></b-form-input>\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <label class=\"mt-3\">Product Cost Price</label>\r\n                <b-form-input for=\"text\" value=\"Cost Price\"></b-form-input>\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <label class=\"mt-3\">Product Sale Price</label>\r\n                <b-form-input for=\"text\" value=\"Sale Price\"></b-form-input>\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <label class=\"mt-3\">Qty</label>\r\n                <b-form-input for=\"text\" value=\"Qty\"></b-form-input>\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <label class=\"mt-3\">SKU</label>\r\n                <b-form-input for=\"text\" value=\"SKU\"></b-form-input>\r\n              </div>\r\n              <div class=\"col-4\">\r\n                <label class=\"mt-3\">EAN</label>\r\n                <b-form-input for=\"text\" value=\"EAN\"></b-form-input>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n          <h4 class=\"card-title mt-3\">Short Description</h4>\r\n          <div class=\"row\">\r\n            <div class=\"col-12\">\r\n              <ckeditor :editor=\"editor\"></ckeditor>\r\n            </div>\r\n          </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mt-3\">Full Product Description</h4>\r\n            <div class=\"row\">\r\n              <div class=\"col-12\">\r\n                <ckeditor :editor=\"editor\"></ckeditor>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mt-3\">Images</h4>\r\n            <div class=\"row\">\r\n              <div class=\"col-3\">\r\n                <vue-dropzone\r\n                  id=\"dropzone\"\r\n                  ref=\"myVueDropzone\"\r\n                  :use-custom-slot=\"true\"\r\n                  :options=\"dropzoneOptions\"\r\n                  :url=\"outputUrl\"  \r\n                >\r\n                  <div class=\"dropzone-custom-content\">\r\n                    <i class=\"display-4 text-muted bx bxs-cloud-upload\"></i>\r\n                    <h4>Drop files here or click to upload.</h4>\r\n                  </div>\r\n                </vue-dropzone>\r\n              </div>\r\n              <div class=\"col-9\">\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n                <div class=\"imageplaceholder\"></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mt-3\">Attributes</h4>\r\n            <div class=\"row\">\r\n              <div class=\"col-12\">\r\n                <div class=\"table-responsive\">\r\n                  <table class=\"table mb-0\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th>Attribute</th>\r\n                        <th>Value</th>\r\n                        <th></th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr>\r\n                        <td>Manufacturer</td>\r\n                        <td>\r\n                          <select class=\"custom-select\">\r\n                            <option value=\"0\">Manufacturer 1</option>\r\n                            <option value=\"1\">Manufacturer 2</option>\r\n                            <option value=\"2\">Manufacturer 3</option>\r\n                            <option value=\"3\">Manufacturer 4</option>\r\n                          </select>\r\n                        </td>\r\n                        <td></td>\r\n                      </tr>\r\n                      <tr>\r\n                        <td>Length</td>\r\n                        <td>\r\n                          <b-form-input for=\"text\" value=\"Length\"></b-form-input>\r\n                        </td>\r\n                        <td></td>\r\n                      </tr>\r\n                      <tr>\r\n                        <td>Width</td>\r\n                        <td>\r\n                          <b-form-input for=\"text\" value=\"Width\"></b-form-input>\r\n                        </td>\r\n                        <td></td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mt-3\">Product Variations</h4>\r\n            <div class=\"variationHeadings\" v-if=\"variations.length\">\r\n              <div class=\"row\">\r\n                <div class=\"col-3\">\r\n                  <label>Variation Name</label>\r\n                </div>\r\n                <div class=\"col-7\">\r\n                  <label>Option Value</label>\r\n                </div>\r\n                <div class=\"col-1\">\r\n                  <label>Required</label>\r\n                </div>\r\n                <div class=\"col-1\">\r\n                  <label>Actions</label>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"variation\" v-for=\"(item, index) of variations\" :key=\"item.index\">\r\n              <div class=\"row\">\r\n                <div class=\"col-3\">\r\n                  <b-form-input value=\"\" size=\"md\" v-model=\"item.variationValue\"></b-form-input>\r\n                </div>\r\n                <div class=\"col-7 mb-1\">\r\n                  <multiselect v-model=\"item.variationOptions\"  :options=\"attributevalues\" ></multiselect>\r\n                </div>\r\n                <div class=\"col-1\">\r\n                  <b-form-checkbox switch size=\"lg\" v-model=\"item.variationRequired\"></b-form-checkbox>\r\n                </div>\r\n                <div class=\"col-1\">\r\n                  <i @click=\"deleteVariation(index)\" class=\"bx bx-trash-alt cursor-pointer\"></i>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"variationToolbar\">\r\n              <div class=\"row\">\r\n                <div class=\"col-12\" v-if=\"variations.length < 4\">\r\n                  <b-button @click=\"addVariation()\" variant=\"primary mb-2 \">\r\n                      <i class=\"bx bx-plus-circle font-size-16 align-middle mr-2\"></i>\r\n                      Add Variation\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"variationGenerated\">\r\n              <div class=\"category-tabs\" role=\"tablist\">\r\n                  <div v-for=\"(variation, index) in variationsData\" :key=\"variation.id\">\r\n                    <b-card no-body class=\"mb-1 shadow-none\">\r\n                      <b-card-header header-tag=\"header\" role=\"tab\">\r\n                        <h6 class=\"m-0\">\r\n                          <span class=\"move\"><i class=\"bx bx-move-vertical\"></i></span>\r\n                          <a\r\n                            v-b-toggle=\"'accordion-' + index\"\r\n                            class=\"text-dark\"\r\n                            href=\"javascript: void(0);\"\r\n                          >{{variation.name}}\r\n                          <div v-for=\"subitem in variation.subitems\" :key=\"subitem.id\">\r\n                            <div class=\"row\">\r\n                              <div class=\"col-4\">\r\n                                <label class=\"mt-3\">Price</label>\r\n                                <b-form-input for=\"text\" :value=\"subitem.price\"></b-form-input>\r\n                              </div>\r\n                              <div class=\"col-4\">\r\n                                <label class=\"mt-3\">Qty</label>\r\n                                <b-form-input for=\"text\" :value=\"subitem.qty\"></b-form-input>\r\n                              </div>\r\n                              <div class=\"col-4\">\r\n                                <label class=\"mt-3\">SKU</label>\r\n                                <b-form-input for=\"text\" :value=\"subitem.sku\"></b-form-input>\r\n                              </div>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                          </a>\r\n                        </h6>\r\n                      </b-card-header>\r\n                      <b-collapse :id=\"'accordion-' + index\" accordion=\"\" role=\"tabpanel\">\r\n                          <div v-for=\"subitem in variation.subitems\" :key=\"subitem.id\">\r\n                            <div class=\"subcategory card-header\">\r\n                              <div class=\"row\">\r\n                                <div class=\"col-3\">\r\n                                  <label class=\"mt-3\">Cost Price</label>\r\n                                  <b-form-input for=\"text\" :value=\"subitem.costprice\"></b-form-input>\r\n                                </div>\r\n                                <div class=\"col-3\">\r\n                                  <label class=\"mt-3\">Sale Price</label>\r\n                                  <b-form-input for=\"text\" :value=\"subitem.saleprice\"></b-form-input>\r\n                                </div>\r\n                                <div class=\"col-3\">\r\n                                  <label class=\"mt-3\">EAN</label>\r\n                                  <b-form-input for=\"text\" :value=\"subitem.ean\"></b-form-input>\r\n                                </div>\r\n                                <div class=\"col-3\">\r\n                                  <label class=\"mt-3\">Custom Image</label>\r\n                                  <vue-dropzone\r\n                                    id=\"dropzone\"\r\n                                    ref=\"myVueDropzone\"\r\n                                    :use-custom-slot=\"true\"\r\n                                    :options=\"dropzoneOptions\"\r\n                                    :url=\"outputUrl\"\r\n                                    >\r\n                                    <div class=\"dropzone-custom-content customImage\">\r\n                                      <i class=\"display-14 text-muted bx bxs-cloud-upload\"></i>\r\n                                      <h6>Drop files here or click to upload.</h6>\r\n                                    </div>\r\n                                  </vue-dropzone>\r\n                                </div>\r\n                                <div class=\"col-12\">\r\n                                  <label class=\"mt-3\">Custom Specs</label>\r\n                                  <div class=\"table-responsive\">\r\n                                    <table class=\"table table-striped mb-0\">\r\n                                      <thead>\r\n                                        <tr>\r\n                                          <th>Attribute</th>\r\n                                          <th>Value</th>\r\n                                        </tr>\r\n                                      </thead>\r\n                                      <tbody>\r\n                                        <tr>\r\n                                          <td>{{subitem.specs[0].name}}</td>\r\n                                          <td>{{subitem.specs[0].value}}</td>\r\n                                        </tr>\r\n                                        <tr>\r\n                                          <td>{{subitem.specs[1].name}}</td>\r\n                                          <td>{{subitem.specs[1].value}}</td>\r\n                                        </tr>\r\n                                      </tbody>\r\n                                    </table>\r\n                                  </div>\r\n                                </div>\r\n                              </div>\r\n                            </div>\r\n                          </div>\r\n                      </b-collapse>\r\n                    </b-card>\r\n                  </div>\r\n                </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mt-3\">Product Bundles</h4>\r\n                <div class=\"row mt-4\">\r\n                        <div class=\"col-sm-12 col-md-6\">\r\n                            <div id=\"tickets-table_length\" class=\"dataTables_length\">\r\n                                <label class=\"d-inline-flex align-items-center\">\r\n                                    Show&nbsp;\r\n                                    <b-form-select v-model=\"perPage\" size=\"sm\" :options=\"pageOptions\"></b-fo