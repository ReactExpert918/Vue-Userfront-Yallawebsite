../layouts/main\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport axios from \"axios\";\r\nimport { attributesData } from \"./attributes-data\";\r\nimport appConfig from \"@/app.config\";\r\n\r\nimport draggable from 'vuedraggable'\r\n\r\n/**\r\n * Pages component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Attributes\",\r\n    meta: [{ name: \"description\", content: appConfig.description }]\r\n  },\r\n  components: { Layout, PageHeader, draggable, },\r\n  data() {\r\n    return {\r\n      primarycheck: null, \r\n      lgchecked: null,\r\n\r\n      id: 1,\r\n      currentAttribut: attributesData[0],\r\n\r\n      selectedAll: false,\r\n\r\n      attributesData: attributesData,\r\n      title: \"Attributes\",\r\n      items: [\r\n        {\r\n          text: \"Catalog\",\r\n          active: true\r\n        },\r\n        {\r\n          text: \"Attributes\",\r\n          active: true\r\n        }\r\n      ],\r\n      totalRows: 1,\r\n      currentPage: 1,\r\n      perPage: 10,\r\n      pageOptions: [10, 25, 50, 100],\r\n      filter: null,\r\n      filterOn: [],\r\n      selected: [],\r\n      isCheckAll: false,\r\n      sortBy: \"age\",\r\n      sortDesc: false,\r\n      fields: [{\r\n              label: \"\",\r\n              key: \"selected\",\r\n              sortable: false,\r\n          },\r\n          {\r\n              label: \"Attribute Name\",\r\n              key: \"attributeName\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              label: \"Is Required\",\r\n              key: \"isRequired\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              label: \"Type\",\r\n              key: \"attributeType\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              label: \"Options\",\r\n              key: \"noOfOptions\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"status\",\r\n              sortable: true,\r\n          },\r\n          {\r\n              key: \"actions\",\r\n          },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n      /**\r\n        * Total no. of records\r\n        */\r\n      rows() {\r\n          return this.attributesData.length;\r\n      },\r\n  },\r\n  watch: {\r\n    selectedAll: function() {\r\n      const selectedLength = this.attributesData.filter(data => data.selected);\r\n      return this.attributesData.forEach( e => {\r\n        if(this.selectedAll === true) { e.selected = true; }\r\n        else if (selectedLength.length === this.attributesData.length) {\r\n          e.selected = false;\r\n        }\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n      // Set the initial number of items\r\n      this.totalRows = this.items.length;\r\n      axios\r\n          .get(\"http://dummy.restapiexample.com/api/v1/employees\", {\r\n              headers: {\r\n                  \"Content-type\": \"application/json;charset=utf-8\",\r\n              },\r\n          })\r\n          .then((res) => {\r\n              return res;\r\n          });\r\n  },\r\n  methods: {\r\n      /**\r\n        * Search the table data with search input\r\n        */\r\n       uncheckSelectAll(){\r\n         this.selectedAll = false\r\n       },\r\n      onFiltered(filteredItems) {\r\n          // Trigger pagination to update the number of buttons/pages due to filtering\r\n          this.totalRows = filteredItems.length;\r\n          this.currentPage = 1;\r\n      },\r\n      addDataToModal(id){\r\n        this.currentAttribut = this.attributesData[id]\r\n      }\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"row mb-2\">\r\n              <div class=\"col-sm-12\">\r\n                <div class=\"text-sm-right\">\r\n                  <b-button v-b-modal.modal-scrollable variant=\"primary\">\r\n                      <i class=\"mdi mdi-plus mr-1\"></i> Add Attribute\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <!-- end col-->\r\n            </div>\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table table-centered table-nowrap\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>#</th>\r\n                    <th>Attribute Name</th>\r\n                    <th>Is Required</th>\r\n                    <th>Type</th>\r\n                    <th>Options</th>\r\n                    <th>Status</th>\r\n                    <th>Actions</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\" (attribute) in attributesData\" :key=\"attribute.id\">\r\n                    <td>\r\n                      <div class=\"custom-control custom-checkbox\">\r\n                        <input\r\n                          :id=\"`customCheck${attribute.id}`\"\r\n                          type=\"checkbox\"\r\n                          class=\"custom-control-input\"\r\n                        />\r\n                        <label\r\n                          class=\"custom-control-label\"\r\n                          :for=\"`customCheck${attribute.id}`\"\r\n                        >&nbsp;</label>\r\n                      </div>\r\n                    </td>\r\n                    <td>{{attribute.attributeName}}</td>\r\n                    <td>{{attribute.isRequired}}</td>\r\n                    <td>{{attribute.attributeType}}</td>\r\n                    <td>{{attribute.noOfOptions}}</td>\r\n                    <td>\r\n                      <span class=\"badge badge-success font-size-12\">\r\n                        <i class=\"mdi mdi-star mr-1\"></i>\r\n                        {{attribute.status}}\r\n                      </span>\r\n                    </td>\r\n                    <td>\r\n                      <b-dropdown class=\"card-drop\" variant=\"white\" right toggle-class=\"p-0\">\r\n                        <template v-slot:button-content>\r\n                          <i class=\"mdi mdi-dots-horizontal font-size-18\"></i>\r\n                        </template>\r\n\r\n                        <b-dropdown-item \r\n                        v-b-modal.modal-scrollable variant=\"primary\"\r\n                        @click=\"currentAttribut = attribute\"\r\n                        >\r\n                          <i class=\"fas fa-pencil-alt text-success mr-1\" ></i> Edit\r\n                        </b-dropdown-item>\r\n\r\n                        <b-dropdown-item>\r\n                          <i class=\"fas fa-trash-alt text-danger mr-1\"></i> Delete\r\n                        </b-dropdown-item>\r\n                      </b-dropdown>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n            <ul class=\"pagination pagination-rounded justify-content-end mb-2\">\r\n              <li class=\"page-item disabled\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\" aria-label=\"Previous\">\r\n                  <i class=\"mdi mdi-chevron-left\"></i>\r\n                </a>\r\n              </li>\r\n              <li class=\"page-item active\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\">1</a>\r\n              </li>\r\n              <li class=\"page-item\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\">2</a>\r\n              </li>\r\n              <li class=\"page-item\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\">3</a>\r\n              </li>\r\n              <li class=\"page-item\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\">4</a>\r\n              </li>\r\n              <li class=\"page-item\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\">5</a>\r\n              </li>\r\n              <li class=\"page-item\">\r\n                <a class=\"page-link\" href=\"javascript: void(0);\" aria-label=\"Next\">\r\n                  <i class=\"mdi mdi-chevron-right\"></i>\r\n                </a>\r\n              </li>\r\n            </ul>\r\n            <!-- <b-button v-b-modal.modal-scrollable variant=\"primary\">Edit Popup</b-button> -->\r\n\r\n            <b-modal id=\"modal-scrollable\" scrollable title=\"Edit Attribute\" title-class=\"font-18\" hide-footer>\r\n              <b-tabs justified nav-class=\"nav-tabs-custom\" content-class=\"p-3 text-muted\">\r\n                <b-tab active>\r\n                  <template v-slot:title>\r\n                    <span class=\"d-inline-block d-sm-none\">\r\n                      <i class=\"fas fa-home\"></i>\r\n                    </span>\r\n                    <span class=\"d-none d-sm-inline-block\">General</span>\r\n                  </template>\r\n                  <div class=\"row\">\r\n                    <div class=\"col-sm-6\">\r\n                      <label class=\"mt-3\">Attribute Name</label>\r\n                      <b-form-input for=\"text\" v-model=\"currentAttribut.attributeName\"></b-form-input>\r\n                      <label class=\"mt-3\">Is Required</label>\r\n                      <b-form-input for=\"text\" v-model=\"currentAttribut.isRequired\"></b-form-input>\r\n                      <label class=\"mt-3\">Options</label>\r\n                      <b-form-input for=\"text\" v-model=\"currentAttribut.noOfOptions\"></b-form-input>\r\n                      <label class=\"mt-3\">Option Type</label>\r\n                      <select class=\"custom-select\">\r\n                        <option value=\"0\">Text</option>\r\n                        <option value=\"1\">Dropdown</option>\r\n                        <option value=\"2\">Checkbox</option>\r\n                        <option value=\"3\">Date</option>\r\n                      </select>\r\n                      <label class=\"mt-3\">Attribute Code</label>\r\n                      <b-form-input for=\"text\" v-model=\"currentAttribut.noOfOptions\"></b-form-input>\r\n\r\n\r\n                    </div>\r\n                    <div class=\"col-sm-6\">\r\n                      <label class=\"mt-3\">Enabled</label>\r\n                      <b-form-checkbox \r\n                        switch size=\"lg\"       \r\n                        value=\"Enabled\"\r\n                        unchecked-value=\"Disabled\" \r\n                        v-model=\"currentAttribut.status\"\r\n                      ></b-form-checkbox>\r\n                    </div>\r\n                  </div>\r\n                </b-tab>\r\n                <b-tab>\r\n                  <template v-slot:title>\r\n                    <span class=\"d-inline-block d-sm-none\">\r\n                      <i class=\"far fa-user\"></i>\r\n                    </span>\r\n                    <span class=\"d-none d-sm-inline-block\">Options</span>\r\n                  </template>\r\n                  <draggable \r\n                    v-model=\"currentAttribut.attributeValues[0].attributeOptions\" \r\n                    group=\"people\" \r\n                    @start=\"drag=true\" \r\n                    @end=\"drag=false\"\r\n                  >\r\n                    <div \r\n                      v-for=\"(attributeoption) of currentAttribut.attributeValues[0].attributeOptions\" \r\n                      :key=\"attributeoption.id\"\r\n                    >\r\n                      <b-card no-body class=\"mb-1 shadow-none\">\r\n                        <b-card-header header-tag=\"header\" >\r\n                          <h6 class=\"row\">\r\n                            <div class=\"col-sm-8\">\r\n                              <span class=\"move\"><i class=\"bx bx-move-vertical\"></i></span>\r\n                              {{attributeoption.optionName}}\r\n                            </div>\r\n                            <div class=\"col-sm-4\">\r\n                              <span class=\"actions-right\">\r\n                                <div class=\"row\">\r\n                                  <div class=\"col-sm-8\">\r\n                                    <b-form-checkbox\r\n                                      class=\"custom-checkbox-primary\"\r\n                                      checked\r\n                                    >Default</b-form-checkbox>\r\n                                  </div>\r\n                                  <div class=\"col-sm-4\">\r\n                                    <i class=\"bx bx-trash-alt\"></i>\r\n                                  </div>\r\n                                </div>\r\n                                \r\n                                </span>\r\n                              </div>\r\n                          </h6>\r\n                        </b-card-header>\r\n                      </b-card>\r\n                    </div>\r\n                  </draggable>\r\n                </b-tab>\r\n                <b-tab>\r\n                  <template v-slot:title>\r\n                    <span class=\"d-inline-block d-sm-none\">\r\n                      <i class=\"far fa-user\"></i>\r\n                    </span>\r\n                    <span class=\"d-none d-sm-inline-block\">Frontend Settings</span>\r\n                  </template>\r\n                  <div class=\"row\">\r\n                    <div class=\"col-sm-6\">\r\n                      <label class=\"mt-3\">Use in Category Filters</label>\r\n                      <b-form-checkbox switch size=\"lg\" v-model=\"lgchecked\"></b-form-checkbox>\r\n                      <label class=\"mt-3\">Show On Product Page</label>\r\n                      <b-form-checkbox switch size=\"lg\" v-model=\"lgchecked\"></b-form-checkbox>\r\n                      <label class=\"mt-3\">Show On Category Page</label>\r\n                      <b-form-checkbox switch size=\"lg\" v-model=\"lgchecked\"></b-form-checkbox>\r\n                      <label class=\"mt-3\">Sort Order</label>\r\n                      <b-form-input for=\"text\" value=\"0\"></b-form-input>\r\n                    </div>\r\n                    <div class=\"col-sm-6\">\r\n                      <label class=\"mt-3\">Show In Product Specifications</label>\r\n                      <b-form-checkbox switch size=\"lg\" v-model=\"lgchecked\"></b-form-checkbox>\r\n                    </div>\r\n                  </div>\r\n                </b-tab>\r\n              </b-tabs>\r\n              <div class=\"text-sm-right\">\r\n                <b-button variant=\"primary\">\r\n                      <i class=\"bx bx-check-double font-size-16 align-middle mr-2\"></i>\r\n                      Publish\r\n                  </b-button>\r\n              </div>\r\n            </b-modal>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"],"sourceRoot":"src/router/views/catalog/attributes"}]}