ut>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    \r\n    <div class=\"row\">\r\n      <div class=\"col-xl-8\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"col-lg-12\">\r\n              <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                  <div class=\"category-tabs\" role=\"tablist\">\r\n\r\n                  <div v-for=\"(category, index) in categoriesData\" :key=\"category.id\">\r\n                    <b-card no-body class=\"mb-1 shadow-none\">\r\n                      <b-card-header header-tag=\"header\" role=\"tab\">\r\n                        <h6 class=\"m-0\">\r\n                          <span class=\"move\"><i class=\"bx bx-move-vertical\"></i></span>\r\n                          <a\r\n                            v-b-toggle=\"'accordion-' + index\"\r\n                            class=\"text-dark\"\r\n                            href=\"javascript: void(0);\"\r\n                          >{{category.name}}<span class=\"actions-right\"><i class=\"bx bx-edit-alt\"></i></span></a>\r\n                        </h6>\r\n                      </b-card-header>\r\n                      <b-collapse :id=\"'accordion-' + index\" accordion=\"\" role=\"tabpanel\">\r\n                          <div v-for=\"subitem in category.subitems\" :key=\"subitem.id\">\r\n                            <div class=\"subcategory card-header\"><span class=\"move\"><i class=\"bx bx-move-vertical\"></i></span>{{ subitem.subcategory }}<span class=\"actions-right\"><i class=\"bx bx-edit-alt\"></i></span></div>\r\n                          </div>\r\n                      </b-collapse>\r\n                    </b-card>\r\n                  </div>\r\n\r\n\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <label>Products In Category</label>\r\n              <table class=\"table mb-0\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>Sort</th>\r\n                    <th>Product Name</th>\r\n                    <th>SKU</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr>\r\n                    <th scope=\"row\"><input type=\"text\" class=\"form-control\" value=\"1\" /></th>\r\n                    <td>Product 1</td>\r\n                    <td>FASJ3-1</td>\r\n                  </tr>\r\n                  <tr>\r\n                    <th scope=\"row\"><input type=\"text\" class=\"form-control\" value=\"2\" /></th>\r\n                    <td>Product 2</td>\r\n                    <td>SALJKG3</td>\r\n                  </tr>\r\n                  <tr>\r\n                    <th scope=\"row\"><input type=\"text\" class=\"form-control\" value=\"3\" /></th>\r\n                    <td>Product 3</td>\r\n                    <td>FDSJ40-1</td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-xl-4\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"mt-4\">\r\n              <div class=\"input-group mb-3\">\r\n                <div class=\"input-group-prepend\">\r\n                  <label class=\"input-group-text\">Category Name</label>\r\n                </div>\r\n                <input type=\"text\" class=\"form-control\" />\r\n              </div>\r\n              <div class=\"input-group mb-3\">\r\n                <div class=\"input-group-prepend\">\r\n                  <label class=\"input-group-text\">Content</label>\r\n                </div>\r\n                <textarea type=\"text\" class=\"form-control\"></textarea>\r\n              </div>\r\n              <div class=\"input-group mb-3\">\r\n                <div class=\"input-group-prepend\">\r\n                  <label>Image</label>\r\n                </div>\r\n                <div id=\"dropzone\" class=\"vue-dropzone dropzone dz-clickable\"><div class=\"dz-message\"><div class=\"dropzone-custom-content\"><div class=\"mb-1\"><i class=\"display-4 text-muted bx bxs-cloud-upload\"></i></div><h4>Drop files here or click to upload.</h4></div></div></div>\r\n              </div>\r\n              <div class=\"input-group mb-3\">\r\n                <div class=\"input-group-prepend\">\r\n                  <label class=\"input-group-text\">Meta Title</label>\r\n                </div>\r\n                <input type=\"text\" class=\"form-control\" />\r\n              </div>\r\n              <div class=\"input-group mb-3\">\r\n                <div class=\"input-group-prepend\">\r\n                  <label class=\"input-group-text\">Meta Keywords</label>\r\n                </div>\r\n                <input type=\"text\" class=\"form-control\" />\r\n              </div>\r\n              <div class=\"input-group mb-3\">\r\n                <div class=\"input-group-prepend\">\r\n                  <label class=\"input-group-text\">Meta Description</label>\r\n                </div>\r\n                <textarea type=\"text\" class=\"form-control\"></textarea>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n\r\n  </Layout>\r\n</template>"]}]}