wrapper-class=\"col-sm-3\" content-class=\"pt-0 px-2 text-muted\">\r\n                  <b-tab title=\"Payment 1\" active title-item-class=\"mb-2\">\r\n                    <b-card-text>\r\n                      <div class=\"row\">\r\n                        <div class=\"col-sm-5\">\r\n                          <label class=\"mt-3\">Card Number</label>\r\n                          <b-form-input for=\"text\"></b-form-input>\r\n                        </div>\r\n                        <div class=\"col-sm-2\">\r\n                          <label class=\"mt-3\">Sort Code</label>\r\n                          <b-form-input for=\"text\"></b-form-input>\r\n                        </div>\r\n                        <div class=\"col-sm-2\">\r\n                          <label class=\"mt-3\">Expiry</label>\r\n                          <b-form-input for=\"text\"></b-form-input>\r\n                        </div>\r\n                        <div class=\"col-sm-2\">\r\n                          <label class=\"mt-3\">CVV</label>\r\n                          <b-form-input for=\"text\"></b-form-input>\r\n                        </div>\r\n                      </div>\r\n                    </b-card-text>\r\n                  </b-tab>\r\n                  <b-tab title=\"Payment 2\" title-item-class=\"mb-2\">\r\n                    <b-card-text>\r\n                      <div class=\"col-sm-12\">\r\n                        <b-button variant=\"primary\">\r\n                            <i class=\"bx bx-check-double font-size-16 align-middle mr-2\"></i>\r\n                            Pay With Paypal\r\n                        </b-button>\r\n                      </div>\r\n                    </b-card-text>\r\n                  </b-tab>\r\n                </b-tabs>\r\n              </div>\r\n            </div>\r\n            <div class=\"row card-body\">\r\n              <div class=\"col-sm-12 text-sm-right\">\r\n                <b-button variant=\"primary\">\r\n                    <i class=\"bx bx-check-double font-size-16 align-middle mr-2\"></i>\r\n                    Save Order\r\n                </b-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- end row -->\r\n  </Layout>\r\n</template>\r\n"]}]}