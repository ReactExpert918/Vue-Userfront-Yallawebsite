put>\r\n                  </div>\r\n                  <div class=\"col-sm-2\">\r\n                    <label class=\"mt-3\">Customer Group</label>\r\n                    <select class=\"custom-select\">\r\n                      <option value=\"0\">Group 1</option>\r\n                      <option value=\"1\">Group 2</option>\r\n                      <option value=\"2\">Group 3</option>\r\n                      <option value=\"3\">Group 4</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"col-sm-1\">\r\n                    <label class=\"mt-3\">Newsletter Subscriber</label>\r\n                    <b-form-checkbox switch size=\"lg\" v-model=\"lgchecked\"></b-form-checkbox>\r\n                  </div>\r\n                  <div class=\"col-sm-3\">\r\n                    <label class=\"mt-3\">New Password</label>\r\n                    <b-form-input for=\"text\"></b-form-input>\r\n                  </div>\r\n                  <div class=\"col-sm-3\">\r\n                    <label class=\"mt-3\">Confirm Password</label>\r\n                    <b-form-input for=\"text\"></b-form-input>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row card-body\">\r\n              <div class=\"col-sm-12\">\r\n                <h4>Addresses</h4>\r\n                <div class=\"row\">\r\n                  <div class=\"col-sm-6\">\r\n                    <h5>Billing Address</h5>\r\n                    <div class=\"row\">\r\n                      <div class=\"col-sm-6\">\r\n                        <div class=\"grey-box\" v-for=\"item in customer.billingAddresses\" :key=\"item.id\">\r\n                          <p>\r\n                            {{item.billingAddressCustomerName}}<br>\r\n                            {{item.billingAddressStreetAddress}}<br>\r\n                            {{item.billingAddressCity}}<br>\r\n                            {{item.billingAddressPostCodeZip}}<br>\r\n                            {{item.billingAddressCountry}}<br>\r\n                            {{item.billingAddressTelephoneNumber}}<br>\r\n                          </p>\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"col-sm-6\">\r\n                        <div class=\"grey-box\" v-for=\"item in customer.billingAddresses\" :key=\"item.id\">\r\n                          <p>\r\n                            {{item.billingAddressCustomerName}}<br>\r\n                            {{item.billingAddressStreetAddress}}<br>\r\n                            {{item.billingAddressCity}}<br>\r\n                            {{item.billingAddressPostCodeZip}}<br>\r\n                            {{item.billingAddressCountry}}<br>\r\n                            {{item.billingAddressTelephoneNumber}}<br>\r\n                          </p>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <h5>Shipping Address</h5>\r\n                    <div class=\"row\">\r\n                      <div class=\"col-sm-6\">\r\n                        <div class=\"grey-box\" v-for=\"item in customer.shippingAddresses\" :key=\"item.id\">\r\n                          <p>\r\n                            {{item.shippingAddressCustomerName}}<br>\r\n                            {{item.shippingAddressStreetAddress}}<br>\r\n                            {{item.shippingAddressCity}}<br>\r\n                            {{item.shippingAddressPostCodeZip}}<br>\r\n                            {{item.shippingAddressCountry}}<br>\r\n                            {{item.shippingAddressTelephoneNumber}}<br>\r\n                          </p>\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"col-sm-6\">\r\n                        <div class=\"grey-box\" v-for=\"item in customer.shippingAddresses\" :key=\"item.id\">\r\n                          <p>\r\n                            {{item.shippingAddressCustomerName}}<br>\r\n                            {{item.shippingAddressStreetAddress}}<br>\r\n                            {{item.shippingAddressCity}}<br>\r\n                            {{item.shippingAddressPostCodeZip}}<br>\r\n                            {{item.shippingAddressCountry}}<br>\r\n                            {{item.shippingAddressTelephoneNumber}}<br>\r\n                          </p>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row card-body\">\r\n              <div class=\"col-sm-12\">\r\n                <h4>Previous Orders</h4>\r\n                <table class=\"table mb-0\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th>Order ID</th>\r\n                      <th>Purchase Date</th>\r\n                      <th>No Of Products</th>\r\n                      <th>Total</th>\r\n                      <th></th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"order in customer.previousOrders\" :key=\"order.id\">\r\n                      <td>{{order.orderId}}</td>\r\n                      <td>{{order.purchaseDate}}</td>\r\n                      <td>{{order.noOfProducts}}</td>\r\n                      <td>{{order.totalPrice }}</td>\r\n                      <td>\r\n                        <b-button variant=\"primary\">\r\n                            <i class=\"bx bx-check-double font-size-16 align-middle mr-2\"></i>\r\n                            View\r\n                        </b-button>\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n   